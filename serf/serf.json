{
  "tags": {
    "role": "web"
  },
  "interface": "eth0",
  "discover": "app",

  "encrypt_key": "1234567890",
  "log_level": "debug",

  "leave_on_terminate": true,
  "skip_leave_on_interrupt": true,

  "reconnect_interval": "5s",
  "reconnect_timeout": "30m",
  "tombstone_timeout": "30m",

  "event_handlers": [
    "./handle.sh",
    "query:hostname=hostname"
  ]
}
